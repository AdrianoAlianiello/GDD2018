<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Entities" assembly="Entities">
	<class name="Cliente" table="clientes" lazy="false">
		<id name="Id" column="Id" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="Activo" column="activo" not-null="true" />
		<property name="Apellido" column="apellido" not-null="true" length="255" />
		<property name="Cuil" column="cuil" not-null="false" />
		<property name="DomicilioCalle" column="domiciliocalle" not-null="true" length="255" />
		<property name="DomicilioCodPostal" column="domiciliocodpostal" not-null="true" length="255" />
		<property name="DomicilioDepto" column="domiciliodepto" not-null="true" length="255" />
		<property name="DomicilioNro" column="domicilionro" not-null="true" />
		<property name="DomicilioPiso" column="domiciliopiso" not-null="true" />
		<property name="FechaCreacion" column="fechacreacion" not-null="true" />
		<property name="FechaNacimiento" column="fechanacimiento" not-null="true" />
		<property name="Mail" column="mail" not-null="true" length="255" />
		<property name="Nombre" column="nombre" not-null="true" length="255" />
		<property name="Telefono" column="telefono" not-null="true" />
		<property name="TipoDocumento" column="tipodocumento" not-null="true" length="255" />
		<property name="UsuarioId" column="usuarioid" not-null="true" />
	</class>
</hibernate-mapping>
