<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Entities" assembly="Entities">
	<class name="Empresa" table="empresas" lazy="false">
		<id name="Id" column="Id" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="Activa" column="activa" not-null="true" />
		<property name="Ciudad" column="ciudad" not-null="false" length="255" />
		<property name="Cuit" column="cuit" not-null="true" length="255" />
		<property name="DomicilioCalle" column="domiciliocalle" not-null="true" length="50" />
		<property name="DomicilioCodPostal" column="domiciliocodpostal" not-null="true" length="50" />
		<property name="DomicilioDepto" column="domiciliodepto" not-null="false" length="50" />
		<property name="DomicilioNro" column="domicilionro" not-null="true" />
		<property name="DomicilioPiso" column="domiciliopiso" not-null="false" />
		<property name="FechaCreacion" column="fechacreacion" not-null="true" />
		<property name="Mail" column="mail" not-null="true" length="50" />
		<property name="RazonSocial" column="razonsocial" not-null="true" length="255" />
		<property name="Telefono" column="telefono" not-null="true" />
		<property name="UsuarioId" column="usuarioid" not-null="true" />
	</class>
</hibernate-mapping>
